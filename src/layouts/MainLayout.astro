---
import '../styles/global.css';
interface Props {
  title: string;
}
const { title } = Astro.props;
---
<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>{title} | Grita</title>
	</head>
	<body class="bg-gray-50 text-gray-900 antialiased">
		<slot /> </body>
</html>